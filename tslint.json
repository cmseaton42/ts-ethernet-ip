{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-config-prettier"
    ],
    "jsRules": {},
    "rules": {
        "curly": [true, "ignore-same-line"],
        "jsdoc-format": false,
        "ordered-imports": false,
        "object-literal-sort-keys": false,
        "no-bitwise": false
    },
    "rulesDirectory": []
}